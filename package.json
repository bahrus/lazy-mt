{
  "name": "lazy-mt",
  "description": "Lazy load DOM in a flattened structure",
  "version": "0.0.24",
  "main": "lazy-mt.js",
  "module": "lazy-mt.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "package-check": "package-check",
    "serve": "node node_modules/xtal-test/serve.js",
    "doc": "custom-elements-manifest analyze"
  },
  "keywords": [
    "web-components",
    "web-component",
    "custom-elements",
    "custom-element",
    "lazy-load"
  ],
  "exports": {
    ".": "./lazy-mt.js",
    "./lazy-mt.js": "./lazy-mt.js"
  },
  "files": [
    "lazy-mt.*",
    "types.d.ts",
    "custom-elements.json"
  ],
  "types": "./types.d.ts",
  "dependencies": {
    "trans-render": "0.0.418",
    "xtal-element": "0.0.442"
  },
  "devDependencies": {
    "@skypack/package-check": "0.2.2",
    "xtal-test": "0.0.51",
    "@custom-elements-manifest/analyzer": "0.5.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bahrus/lazy-mt.git"
  },
  "author": "anderson.bruce.b@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bahrus/lazy-mt/issues"
  },
  "homepage": "https://github.com/bahrus/lazy-mt#readme",
  "customElements": "custom-elements.json"
}
